@model IEnumerable<HealthCatalyst.Models.Person>


@foreach (var person in Model)
            {
    <tr>
        <td>
            <img src="/Content/images/@person.Photo"
                 title="@Html.DisplayFor(modelItem => person.Name)"
                 width='100'>
        </td>
        <td>
            @Html.DisplayFor(modelItem => person.Name)
        </td>
        <td>@Html.DisplayFor(modelItem => person.Address)</td>
        <td>@Html.DisplayFor(modelItem => person.Age)</td>
        <td>@Html.DisplayFor(modelItem => person.Interests)</td>
    </tr>
}
